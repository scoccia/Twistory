<html>

  <title>Modifica feed</title>

  <%= form_for(@feed) do |f| %>
  
    <%= feed_error_messages! %>

    <div class="sheet">
      <%= f.text_area :feed_text, class: "text_box", autofocus: true %><br><br>
      Inserisci la data in cui verr√† pubblicato (fuso orario UTC):<br><br>
      <%= f.datetime_select :date %><br><br>
      Inserisci un'immagine:<br>
      <%= f.file_field :feed_image %><br><br>
      <%= f.submit %>
    </div>
  
  <% end %>

</html>
